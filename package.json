{
  "name": "@vibecli/core",
  "version": "0.0.1",
  "description": "Advanced AI agent that lives in your terminal with streaming responses and tool integration",
  "main": "dist/index.js",
  "bin": {
    "vibe": "./bin/vibe"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Fenix46/vibecli.git"
  },
  "homepage": "https://github.com/Fenix46/vibecli#readme",
  "bugs": {
    "url": "https://github.com/Fenix46/vibecli/issues"
  },
  "scripts": {
    "build": "tsc",
